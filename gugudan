// a부터 b까지 해당하는 구구단 출력
#include<stdio.h>
int main(){

    int a,b;
    
    while(1){

    printf("첫번째 값을 입력해주십시오.");
    scanf("%d",&a);
    
    printf("두번째 값을 입력해주십시오.");
    scanf("%d",&b);

    if(a>=1 || b>=1){
        break;
    }

    if(a==0 || b==0){
        printf("재입력 바랍니다.\n");
        continue;
    }
    
    }
    if(a > b){
        int c = b;
        for(;a>=b;b++){
            printf("\n%d단\n",c);
        for(int i = 1; i<=9; i++){
            printf("%d\n",c*i);
        }
        c++;
            
        }
    }
    else if(a < b){
    int c = a;
        for(;b>=a;a++){
            printf("\n%d단\n",c);
        for(int i = 1; i<=9; i++){
            printf("%d\n",c*i);
        }
        c++;
            
    }

}


}
